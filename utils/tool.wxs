

module.exports={
   checkEmpty: function(obj){
    if (obj == null || obj==undefined) {
      return true;
    }
    if ((typeof obj == 'string' || obj.constructor === Array) && obj.length <= 0) {
      return true;
    } 
    if (typeof obj == 'object' && Object.keys(obj).length <= 0) {
      return true;
    }
    return false;
  }
}